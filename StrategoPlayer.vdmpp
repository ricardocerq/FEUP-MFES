class StrategoPlayer
	instance variables
		team: Stratego`Team;
		plays: seq of Stratego`Move := [];
		captured: seq of Stratego`Character := [];
		
	operations
		public StrategoPlayer: Stratego`Team ==> StrategoPlayer
			StrategoPlayer(t) == (
				team := t;
				return self;
			)
		;
end StrategoPlayer